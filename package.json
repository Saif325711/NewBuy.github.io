{
  "name": "newbuy-admin-panel",
  "version": "1.0.0",
  "description": "NewBuy Admin Panel - Monorepo",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "storefront": "cd frontend && npm run dev",
    "install-all": "npm install && cd server && npm install && cd client && npm install && cd frontend && npm install",
    "dev": "npm run start"
  },
